import java.util.Scanner;

public class BaiTap1IfElse {
    public static void main(String[] args) {
        //input: Nhập vào số nguyên m và n
        Scanner input = new Scanner(System.in);
        System.out.println("Nhập vào số nguyên m:");
        int m = input.nextInt();
        System.out.println("Nhập vào số nguyên n:");
        int n = input.nextInt();
        //Process + output: Nếu m > n -> m là số lớn nhất và ngược lại. Sau đó in ra màn hình
        System.out.println((m > n) ? ("Số nguyên lớn nhất là " + m) : ("Số nguyên lớn nhất là " + n));
    }
}
