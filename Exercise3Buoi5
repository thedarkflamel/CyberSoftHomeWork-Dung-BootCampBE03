import java.util.Scanner;

public class BaiTap3Buoi5 {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Nhập vào lần lượt 2 số a và b:");
        int a = input.nextInt();
        int b = input.nextInt();
        System.out.println("Tổng 2 số nguyên a và b là: " + (a + b) );
    }
}
